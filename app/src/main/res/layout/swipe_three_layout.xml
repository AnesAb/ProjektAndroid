<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/swipe_title3"
        android:id="@+id/textView_swipe3"
        android:layout_marginTop="122dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="18dp"
        android:layout_height="25dp"
        android:src="@drawable/plupp_gron_ljus"
        android:layout_marginBottom="30dp"
        android:id="@+id/imageView_pos1"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/imageView_pos2"
        android:layout_toStartOf="@+id/imageView_pos2"
        android:layout_marginRight="15dp"
        android:layout_marginEnd="15dp" />

    <ImageView
        android:layout_width="18dp"
        android:layout_height="25dp"
        android:src="@drawable/plupp_gron_ljus"
        android:id="@+id/imageView_pos2"
        android:layout_alignTop="@+id/imageView_pos1"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/plupp_gron"
        android:id="@+id/imageView_pos3"
        android:layout_alignTop="@+id/imageView_pos2"
        android:layout_toRightOf="@+id/imageView_pos2"
        android:layout_toEndOf="@+id/imageView_pos2"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp" />



</RelativeLayout>